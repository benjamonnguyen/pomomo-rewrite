{
	"app": {
		"name": "pomomo-bot"
	},
	"bot": {
		"token": <YOUR_TOKEN>,
		"clientId": <YOUR_CLIENT_ID>
	},
	"bridge": {
		"host": "localhost",
		"port": 4444,
		"authToken": <YOUR_AUTH_TOKEN>,
    		"totalShards": 1,
		"totalMachines": 1,
		"shardsPerCluster": 1
	},
	"voice": {
		"audioPlayer": {
			"pool": {
				"minSize": 1,
				"maxSize": 5,
				"idleTimeoutMs": 300000,
				"idleTimeoutPollRateMs": 600000
			}
		}
	},
	"redis": {
		"db": {
			"sessions": {
				"url": <YOUR_REDIS_URL>
			}
		}
	},
	"api": {
		"port": 3001
	},
	"allowedGuilds": ["*"]
}
